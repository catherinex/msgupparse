<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.parse.starter.contacts.ContactsActivity"
     >

    <EditText
        android:id="@+id/et_contacts_search"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    
    <ImageView
        android:id="@+id/iv_contacts_search"
        android:padding="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/et_contacts_search"
        android:layout_alignBottom="@+id/et_contacts_search"
        android:layout_alignRight="@+id/et_contacts_search"
        android:src="@drawable/ic_action_search" />
    
    <TextView
        android:id="@+id/tv_contacts_my"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="My username: aa"
        android:layout_below="@+id/et_contacts_search"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        />
    
    <FrameLayout
            android:id="@+id/container_contacts"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_below="@+id/tv_contacts_my"
    		android:paddingLeft="@dimen/activity_horizontal_margin"
    		android:paddingRight="@dimen/activity_horizontal_margin"
    		android:paddingTop="@dimen/activity_vertical_margin"
    		android:layout_marginTop="@dimen/activity_vertical_margin"
            />
    
    

</RelativeLayout>
